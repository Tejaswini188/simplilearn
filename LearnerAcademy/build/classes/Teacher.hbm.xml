<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
    <hibernate-mapping package="com.Learner.demo">
    
    <class name="TeacherDetails" table="teacher">
    
    <id name="ID" type="long" column="ID">
    <generator class="increment"></generator>
    </id>
    
	<property name="name" type="string"></property>
	<property name="email" type="string" ></property>
	<property name="gender" type="string"></property>
	<property name="dob" type="string"></property>
	<property name="mobno" type="big_decimal"></property>
	<property name="address" type="string"></property>
	<property name="sub" type="string"></property>
    </class>
    </hibernate-mapping>