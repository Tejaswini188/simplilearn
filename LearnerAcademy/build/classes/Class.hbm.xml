<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
    <hibernate-mapping package="com.Learner.demo">
    
    <class name="ClassDetails" table="class">
    
    <id name="ID" type="long" column="ID">
    <generator class="increment"></generator>
    </id>
    
	<property name="t_ID" type="big_decimal"></property>
	<property name="s_ID" type="big_decimal" ></property>
	<property name="std" type="string"></property>
	<property name="time" type="string"></property>
	<property name="a_ID" type="big_decimal"></property>
    </class>
    </hibernate-mapping>